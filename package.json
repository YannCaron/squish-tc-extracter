{
  "name": "squish-tc-extracter",
  "version": "1.0.0",
  "description": "A tool for Ravens team for extracting Test cases from squish automated tests",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node --loader ts-node/esm index.ts",
    "dev": "npx nodemon index.ts",
    "build": "tsc",
    "test": "jest --watchAll --coverage",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YannCaron/squish-tc-extracter.git"
  },
  "keywords": [
    "squish",
    "test",
    "auto",
    "java",
    "squing",
    "javaFx"
  ],
  "nodemonConfig": {
    "ignore": [
      "generated/package.json"
    ]
  },
  "author": "Yann Caron",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YannCaron/squish-tc-extracter/issues"
  },
  "homepage": "https://github.com/YannCaron/squish-tc-extracter#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.11.17",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/generator": "^7.24.4",
    "@babel/parser": "^7.24.4",
    "@babel/traverse": "^7.24.1",
    "@cyann/js-commons": "^0.2.4",
    "json5": "^2.2.3",
    "jsonpath": "^1.1.1",
    "ts-pattern": "^5.1.1"
  }
}
